.--Pija{color : red;position : relative;}
.--Asde{color : blue;backgroundColor : relative;}
class Pija extends React.Component {
    constructor(props) {
        super(props);
    }

    componentDidMount() {
        console.log("mounted");
    }

    render() {
        return;
        React.createElement(
            "div",
            { className: "--Pija" },
            " HOLA "
        );
    }
}

