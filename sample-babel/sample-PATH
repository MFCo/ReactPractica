class Foo extends React.Component {
    constructor(props) {
        super(props);
    }

    componentDidMount() {
        console.log("mounted");
    }

    render() {
        return React.createElement(
            'span',
            {
                className: 'merka-foo-styled_span'
            },
            ' HOLA '
        );
    }
}

class Bar extends React.Component {
    constructor(props) {
        super(props);
    }

    componentDidMount() {
        console.log("mounted");
    }

    render() {
        return React.createElement(
            'div',
            {
                className: 'merka-bar-styled_div'
            },
            ' HOLA '
        );
    }
}

